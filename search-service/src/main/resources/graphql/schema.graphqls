
scalar Long

type Query {
    languageById(id: Long): LanguageResponse
    languages: [LanguageResponse]
}

type Mutation {
     addLanguage(code: String!, name: String!): LanguageResponse
     updateUser(id: Long!, name: String, code: String): LanguageResponse
}

type LanguageResponse{
    id: ID
    name: String
    code: String
}




